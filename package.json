{
  "name": "react-learning-examples",
  "version": "1.0.0",
  "description": "a collection of examples illustrating using React.js",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "tsc": "tsc",
    "start:dev": "npm run build:dev & node server.js",
    "start": "npm run build & node server.js",
    "build:dev": "NODE_ENV=development webpack-dev-server --inline --hot --config webpack.config.js",
    "build": "NODE_ENV=production webpack -p --config webpack.config.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "tictactoe/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 90
      }
    },
    "coverageDirectory": "reports/test_coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpultz/react-learning-examples.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel"
  ],
  "author": "wpultz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wpultz/react-learning-examples/issues"
  },
  "homepage": "https://github.com/wpultz/react-learning-examples#readme",
  "dependencies": {
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "img-loader": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "typescript": "^2.9.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "babel-jest": "^22.1.0",
    "jest": "^22.1.4"
  }
}
